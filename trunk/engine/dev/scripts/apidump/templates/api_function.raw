{$header}

<h1>{$name}</h1>
<div class="box">
	<div class="inner">
		<div class="left-content">
			<fieldset>
				<legend><strong>Meta information</strong></legend>
				<table cellspacing="4" cellpadding="0">
					<tr>
						<td>Availability:</td>
						<td class="value" style="width: 100%">
							Engine ???
						</td>
					</tr>
					<tr>
						<td>Type:</td>
						<td class="value" style="width: 100%">
							Procedural function
						</td>
					</tr>
					<tr>
						<td nowrap="nowrap">Declared in:</td>
						<td class="value" style="width: 100%">
							{$file}
						</td>
					</tr>
					<tr>
						<td>Namespace:</td>
						<td class="value" style="width: 100%">
							{$namespace}
						</td>
					</tr>
				</table>
			</fieldset>
		</div>
		<div class="content">
			<fieldset>
				<legend>Prototype</legend>
				<table cellspacing="4" cellpadding="0" style="width: 100%;">
					<tr>
						<td class="value" style="width: 100%"><code>{$prototype}</code></td>
					</tr>
				</table>
			</fieldset>
			<div class="infobox">
				{$description}
			</div>
			<br />
		</div>
		<div style="clear: left;"></div>
	</div>
</div>

<h2><span>Examples</span></h2>
<div class="box edge-title">
	<div class="inner">
		<fieldset>
			<legend><strong>...</strong></legend>
			<table cellspacing="4" cellpadding="0" style="width: 100%;">
				<tr> 
					<td colspan="2" class="value" style="width: 100%">
						<pre><code>...</code></pre>
					</td>
				</tr>
			</table>
		</fieldset>
	</div>
</div>

<h2><span>Version history</span></h2>
<div class="box edge-title">
	<div class="inner">
		<table style="width: 100%" cellspacing="2" cellpadding="0">
			<tr class="head">
				<td class="strong" nowrap="nowrap">Engine version</td>
				<td class="strong">Notes</td>
			</tr>
			<tr class="row">
				<td nowrap="nowrap">...</td>
				<td nowrap="nowrap" style="width: 100%">...</td>
			</tr>
		</table>
	</div>
</div>

<h2><span>See also</span></h2>
<div class="box edge-title">
	<div class="inner">
		<ul>
			<li><a href="./functions.html">List of functions</a></li>
		</ul>
	</div>
</div>

{$footer}