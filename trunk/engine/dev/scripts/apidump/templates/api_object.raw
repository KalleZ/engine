{$header}

<h1>{$name}</h1>
<div class="box">
	<div class="inner">
		<div class="left-content">
			<fieldset>
				<legend><strong>Meta information</strong></legend>
				<table cellspacing="4" cellpadding="0">
					<tr>
						<td>Availability:</td>
						<td class="value" style="width: 100%">
							{$appname} {$since}
						</td>
					</tr>
					<tr>
						<td>Type:</td>
						<td class="value" style="width: 100%">
							<a href="./{$mtype}.html">{$type}</a>
						</td>
					</tr>
					{$extendedinfo}
				</table>
			</fieldset>
		</div>
		<div class="content">
			<div class="infobox">
				{$description}
			</div>
			<br />
		</div>
		<div style="clear: left;"></div>
	</div>
</div>

{$notices}

{$warnings}

<h2><span>Contents</span></h2>
<div class="box edge-title">
	<div class="inner">
		<div class="infobox">
			Table of contents
		</div>

		{$contents}
	</div>
</div>

{$examples}

{$todo}

{$changelog}

<h2><span>See also</span></h2>
<div class="box edge-title">
	<div class="inner">
		<ul>
			{$seealso}
			<li><a href="./{$mtype}.html">List of {$mtype}</a></li>
			<li><a href="./index.html">Table of contents</a></li>
		</ul>
	</div>
</div>

{$footer}