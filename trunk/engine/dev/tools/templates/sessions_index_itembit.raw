<tr>
	<td>
		{$session->sessionid}
	</td>
<if expression="$session->userid">
	<td>
		{$userinfo->id}
	</td>
	<td>
		{$userinfo->username}
	</td>
	<td>
		{$usergroup}
	</td>
<else />
	<td colspan="3" style="text-align: center;">
		<em>User session not authenticated</em>
	</td>
</if>
	<td>
		{$session->lastactivity}
	</td>
	<td>
		{$session->expires}
	</td>
	<td nowrap="nowrap">
		{$session->location}
	</td>
	<td>
		<a href="./sessions.php?do=kill&action=single&id={$session->sessionid}">Kill</a>
	</td>
</tr>