<tr>
	<td>
		{$opt['option']}
	</td>
	<td nowrap=\"nowrap\">
" . ((!strlen($value)) ? ("
		<em>Default</em>
") : ("
		{$value}
")) . "
	</td>
	<td nowrap=\"nowrap\">
		{$defaultvalue}
	</td>
	<td>
		{$type}
	</td>
	<td>
		" . (($cached) ? ("Yes") : ("No")) . "
	</td>
	<td nowrap=\"nowrap\">
" . (($cached && ($cache->options[$opt['option']] == $opt['defaultvalue'])) ? ("
		<em>Default</em>
") : ("
		{$cachevalue}
")) . "
	</td>
	<td nowrap=\"nowrap\">
		<select onchange=\"engine_options_handler(this, '{$opt['option']}');\">
			<option></option>
			<option value=\"edit\">Edit</option>
			<option value=\"delete\">Delete</option>
			<option value=\"reset\">Reset</option>
		</select>
	</td>
</tr>