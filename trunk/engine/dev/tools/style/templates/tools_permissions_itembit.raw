<tr>
	<td>
		{$name}
	</td>
<if expression="isset($test_permissions)">
	<td nowrap="nowrap">
		<if expression="$user->isGranted($bits)">PASSED<else /><span class="error">FAILED</span></if></td>
<else />
	<td>
		<input type="checkbox" name="permission_{$name}"<if expression="$selected"> checked="checked"</if> /></td>
</if>
</tr>
